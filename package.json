{
  "name": "flow-graphql-relay",
  "version": "0.0.1",
  "description": "graphql-relay(0.4.1) with .js.flow output",
  "contributors": [
    "Daniel Schafer <dschafer@fb.com>"
  ],
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/iamchenxin/flow-graphql-relay",
  "bugs": {
    "url": "https://github.com/iamchenxin/flow-graphql-relay/issues"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/iamchenxin/flow-graphql-relay.git"
  },
  "main": "lib/index.js",
  "directories": {
    "lib": "./lib"
  },
  "files": [
    "lib",
    "README.md",
    "LICENSE",
    "PATENTS"
  ],
  "babel": {
    "optional": [
      "runtime",
      "es7.asyncFunctions",
      "es7.objectRestSpread"
    ]
  },
  "scripts": {
    "build": "rm -rf lib/* && babel src --ignore __tests__ --optional runtime --out-dir lib && gulp flow"
  },
  "dependencies": {
    "babel-runtime": "~5.8.3"
  },
  "peerDependencies": {
    "graphql": "~0.6.0"
  },
  "devDependencies": {
    "babel": "5.8.3",
    "babel-core": "5.8.3",
    "babel-eslint": "5.0.0",
    "babel-preset-fbjs": "^1.0.0",
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "coveralls": "2.11.8",
    "eslint": "2.2.0",
    "flow-bin": "0.22.1",
    "graphql": "~0.6.0",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "isparta": "3.0.3",
    "mocha": "2.2.5",
    "sane": "1.1.3"
  }
}
